{
    "presets": [
        [
            "env",
            {
                "modules": "commonjs",
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
                },
                "debug": false,
                "useBuiltIns": true
            }
        ],
        "stage-2"
    ],
    "plugins": ["transform-runtime", "lodash", "syntax-dynamic-import"],
    "env": {
        "test": {
            "presets": ["env", "stage-2"],
            "plugins": ["istanbul"]
        }
    }
}
